--Library--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Xerces X - Version 1", "Ocean")
--Library End

--Varibles--
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

local Extra = Window:NewTab("Extra")
local ExtraSection = Extra:NewSection("Extra")

local Credits = Window:NewTab("By WinzexTim")
local CreditSection = Credits:NewSection("Credits")
--Varibles End

--ExtraSection Code--

